<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vod">
    <insert id="insert_vod">
        INSERT INTO VOD
        VALUES(#{TITLE}
              ,#{CATEGORY}
              ,#{CONTENT}
              ,#{IMAGEPATH})
    </insert>
    
    <select id="select_vod">
        SELECT *
          FROM VOD
    </select>
</mapper>